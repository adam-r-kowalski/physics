{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-implicit-dependencies": false,
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "trailing-comma": [true, { "multiline": "always", "singleline": "never" }]
  }
}
